<launch>
  <arg name="planning" default="ompl" />
  <!-- By default, we are not in debug mode -->
  <arg name="debug" default="false" />
  <arg name="limited" default="true" />
  <arg name="load_robot_description" default="true" />
  <include file="$(find left_sr_ur10_moveit_config)/launch/planning_context.launch">

    <arg name="load_robot_description"
    value="$(arg load_robot_description)" />
    <arg name="limited" value="$(arg limited)" />
  </include>
  <include file="$(find right_sr_ur10_moveit_config)/launch/move_group.launch">

    <arg name="allow_trajectory_execution" value="true" />
    <arg name="fake_execution" value="false" />
    <arg name="info" value="true" />
    <arg name="debug" value="$(arg debug)" />
    <arg name="publish_monitored_planning_scene" value="true" />
    <arg name="pipeline" value="$(arg planning)" />
  </include>
  <!-- Run Rviz and load the default config to see the state of the move_group node -->
  <include file="$(find right_sr_ur10_moveit_config)/launch/moveit_rviz.launch">

    <arg name="config" value="true" />
    <arg name="debug" value="$(arg debug)" />
  </include>
</launch>
